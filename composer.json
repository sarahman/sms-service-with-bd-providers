{
    "name": "sarahman/sms-service-with-bd-providers",
    "description": "SMS Gateway service library in PHP. Multiple providers of Bangladesh in one library",
    "keywords": ["sms", "gateway", "service", "providers", "framework", "laravel", "lumen"],
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Syed Abidur Rahman",
            "email": "aabid048@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.34",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/config": "^4.0",
        "illuminate/support": "^4.0",
        "illuminate/validation": "^4.0",
        "psr/http-message-implementation": "*",
        "sarahman/laravel-http-request-api-log": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Sarahman\\SmsService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
